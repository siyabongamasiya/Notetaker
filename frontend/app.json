{
  "expo": {
    "name": "Notetaker",
    "slug": "Notetaker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/note-taker-logo-dark.png",
    "scheme": "notetaker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/note-taker-logo-dark.png"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/note-taker-logo-dark.png"
      },
      "icon": "./assets/images/note-taker-logo-dark.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.siyabonga_khanyile.Notetaker"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/note-taker-logo-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/note-taker-logo-dark.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8b5669dd-b017-44d7-84b9-c13a8adb90eb"
      }
    }
  }
}
